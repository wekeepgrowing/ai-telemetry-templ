{
  "name": "ai-telemetry-template",
  "version": "0.1.0",
  "description": "Template for AI telemetry integration with Langfuse",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsx --env-file=.env src/examples/ai-telemetry-demo.ts",
    "dev": "tsx watch --env-file=.env src/examples/ai-telemetry-demo.ts",
    "test": "node --test src/**/*.test.ts"
  },
  "keywords": [
    "ai",
    "telemetry",
    "langfuse",
    "opentelemetry"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.0",
    "@opentelemetry/api-logs": "^0.200.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/instrumentation": "^0.200.0",
    "@opentelemetry/sdk-logs": "^0.200.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "ai": "^4.2.0",
    "js-tiktoken": "^1.0.19",
    "langfuse": "^3.37.0",
    "langfuse-vercel": "^3.37.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
